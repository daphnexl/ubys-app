<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:ubys_app.MVVM.ViewModels"
                    xmlns:view="clr-namespace:ubys_app.MVVM.View">
    <DataTemplate DataType="{x:Type vm:LoginPageViewModel}">
        <view:LoginPageView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AddStudentViewModel}">
        <view:AddStudentView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AddTeacherViewModel}">
        <view:AddTeacherView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AddAdminViewModel}">
        <view:AddAdminView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AddCourseViewModel}">
        <view:AddCourseView />
    </DataTemplate>


</ResourceDictionary>
